<h1>Welcome to Google Map demo!</h1>

{{google-map lat=centerLat
lng=centerLng
zoom=zoom
gopt_zoomControl=false
type='satellite'
markerInfoWindowTemplateName='map/info-window'
markers=model}}

<div class="controls">
  <label>
    Zoom:
    {{input type="range" value=zoom min=0 max=18 step=1}}
  </label>
</div>